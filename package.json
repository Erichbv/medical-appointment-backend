{
  "name": "medical-appointment-backend",
  "version": "1.0.0",
  "description": "Backend para un sistema de agendamiento de citas, diseñado con una arquitectura distribuida que integra API Gateway, SNS, SQS y múltiples bases de datos optimizadas para garantizar alto rendimiento, escalabilidad y resiliencia.",
  "main": "index.js",
  "scripts": {
    "dev": "serverless offline start",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "test": "jest --runInBand",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^30.2.0",
    "prettier": "^3.2.5",
    "prisma": "^6.0.0",
    "serverless": "^4.25.0",
    "serverless-offline": "^14.4.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.939.0",
    "@aws-sdk/client-ec2": "^3.939.0",
    "@aws-sdk/client-eventbridge": "^3.939.0",
    "@aws-sdk/client-rds": "^3.939.0",
    "@aws-sdk/client-sns": "^3.939.0",
    "@aws-sdk/client-sqs": "^3.939.0",
    "@aws-sdk/lib-dynamodb": "^3.939.0",
    "@prisma/client": "^6.0.0",
    "aws-sdk": "^2.1692.0",
    "joi": "^18.0.2",
    "uuid": "^13.0.0"
  },
  "exports": {
    "@prisma/client-pe": "./src/node_modules/.prisma/client-pe/index.js",
    "@prisma/client-cl": "./src/node_modules/.prisma/client-cl/index.js"
  }
